{
  "name": "reactjs-server-side-table",
  "version": "0.1.5",
  "private": false,
  "description": "A server side table for React",
  "main": "./src/table/ServerSideTable.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
